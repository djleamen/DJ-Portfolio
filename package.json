{
  "name": "amplify-vue-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "api": "cd api && PATH=\"/opt/homebrew/opt/node@20/bin:$PATH\" npm start",
    "migrate": "cd scripts && npm run migrate",
    "setup": "./setup.sh"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.12.0",
    "marked": "^15.0.7",
    "mongodb": "^6.20.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.8"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.12.5",
    "@types/vue": "^2.0.0",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "constructs": "10.1.161",
    "esbuild": "^0.25.5",
    "npm-run-all2": "^6.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^6.4.1",
    "vue-tsc": "^2.0.11"
  }
}
